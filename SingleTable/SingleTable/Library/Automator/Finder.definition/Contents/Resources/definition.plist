	<key>scripts</key>
	<dict>
		<key>get selected items</key>
		<string>on run {input, parameters}
		
		set output to input
		set currentSelection to missing value

		tell application &quot;Finder&quot;
			set currentSelection to selection
			
			repeat with i from 1 to count of currentSelection
				set theItem to item i of currentSelection
				copy (theItem as alias) to end of output
			end repeat
			
		end tell

		return output
end run
</string>
